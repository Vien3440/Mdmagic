<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Deckcard" table="deckCard">
    <indexes>
      <index name="fk_deck_has_card_deck_idx" columns="deck_id"/>
    </indexes>
    <id name="deck" association-key="true"/>
    <field name="cardId" type="text" column="card_id" length="16777215" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="number" type="integer" column="number" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="deck" target-entity="Deck" fetch="LAZY">
      <join-columns>
        <join-column name="deck_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
