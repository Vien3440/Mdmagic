<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <unique-constraints>
      <unique-constraint name="pass_UNIQUE" columns="pass"/>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nameuser" type="string" column="nameUser" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pseudo" type="string" column="pseudo" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pass" type="string" column="pass" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imguser" type="string" column="imgUser" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
